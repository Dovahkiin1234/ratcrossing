-- Gui to Lua
-- Version: 3.2

-- Instances:

local islam = Instance.new("ScreenGui")
local open = Instance.new("TextButton")
local mainframe = Instance.new("Frame")
local akbar = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local close = Instance.new("TextButton")
local TextLabel_2 = Instance.new("TextLabel")

--Properties:

islam.Name = "islam"
islam.Parent = game.CoreGui
islam.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

open.Name = "open"
open.Parent = islam
open.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
open.Position = UDim2.new(0, 0, 0.956632674, 0)
open.Size = UDim2.new(0, 126, 0, 34)
open.Style = Enum.ButtonStyle.RobloxButtonDefault
open.Font = Enum.Font.GothamSemibold
open.Text = "OPEN"
open.TextColor3 = Color3.fromRGB(0, 255, 17)
open.TextSize = 14.000
open.MouseButton1Down:connect(function()
mainframe.Visible = true
open.Visible = false
end)

mainframe.Name = "mainframe"
mainframe.Parent = islam
mainframe.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mainframe.BackgroundTransparency = 0.350
mainframe.Position = UDim2.new(0.433794439, 0, 0.213010207, 0)
mainframe.Size = UDim2.new(0, 255, 0, 100)

akbar.Name = "akbar"
akbar.Parent = mainframe
akbar.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
akbar.BackgroundTransparency = 0.450
akbar.Position = UDim2.new(0.105882354, 0, 0.319999993, 0)
akbar.Size = UDim2.new(0, 200, 0, 50)
akbar.Font = Enum.Font.GothamSemibold
akbar.Text = "ALLAHU AKBAR!!!"
akbar.TextColor3 = Color3.fromRGB(31, 147, 0)
akbar.TextSize = 20.000
akbar.MouseButton1Down:connect(function()
saymessage = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest

saymessage:FireServer("الحمد لله وجميع مؤيدي الإسلام", "all")
wait(1)
saymessage:FireServer("سيكون في انتظارك", "all")
wait(1)
saymessage:FireServer("بارك الله فيك", "all")
wait(1)
saymessage:FireServer("هل أهنت الله؟", "all")
wait(1)
saymessage:FireServer("هذه لحظة الله الحقيقية", "all")
wait(1)
saymessage:FireServer("الله أعظم", "all")
wait(1)
saymessage:FireServer("هذا المكان رائع", "all")
wait(1)
saymessage:FireServer("هل أنت إسلامي أيضًا؟", "all")
wait(1)
saymessage:FireServer("أنا أؤيد الإسلام الراديكالي", "all")
wait(1)
saymessage:FireServer("هذه لحظة الله", "all")
end)

TextLabel.Parent = mainframe
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
