-- Scripts:

local function MVSJCG_fake_script() -- mainframe.LocalScript 
	local script = Instance.new('LocalScript', mainframe)

	script.parent.Selectable = true
	script.Parent.Active = true
	script.parent.Draggable = true
end
coroutine.wrap(MVSJCG_fake_script)()
local function AWXAMM_fake_script() -- mainframe.LocalScript 
	local script = Instance.new('LocalScript', mainframe)

	game:GetService("StarterGui"):SetCore("SendNotification",{
	Title    = "Made By Chillin aka mac:)",
		Text     = "SCRIPT NOT MADE BY ME BTW ONLY THE GUI",
	Duration = 10,
	})
end
coroutine.wrap(AWXAMM_fake_script)()
